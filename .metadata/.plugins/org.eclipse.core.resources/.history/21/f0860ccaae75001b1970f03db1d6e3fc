package array;

public class StudentArray {
	public static void main(String[] args) {
		Student[] study = new Student[3];	
		study[0] = new Student(1001, "James");
		study[1] = new Student(1002, "Tomas");
		study[2] = new Student(1003, "Edward");
		
		for(int i = 0; i < study.length; i++) {
			System.out.println();
		}
	}
}